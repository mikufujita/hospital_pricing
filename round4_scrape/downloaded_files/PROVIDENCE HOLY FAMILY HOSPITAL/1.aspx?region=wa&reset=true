        (function() {
            var tkn = 'bbf557c2b5d84e559c1f6dd8b56f2b3d';
            var typeaheaduri = 'https://api.kyruus.com/pm/v8/phs/typeahead';
            var regionfilter = 'fields=-ALL%2c%2baccepting_new_patients%2c%2baccess_program_status%2c%2bage_groups_seen%2c%2bawards%2c%2bboard_certifications%2c%2bcase_types_seen%2c%2bclinical_contact%2c%2bclinical_trials%2c%2bcontacts%2c%2bcustomer_code%2c%2bdegrees%2c%2bexternal_id%2c%2bgender%2c%2bgeneric%2c%2bgrants%2c%2bid%2c%2bimage_url%2c%2binsurance_accepted%2c%2bis_live%2c%2bis_primary_care%2c%2bis_specialty_care%2c%2blanguages%2c%2blegal%2c%2blicense%2c%2blocations%2c%2bmax_wait%2c%2bmedia_mentions%2c%2bmedical_school%2c%2bmetadata%2c%2bmin_wait%2c%2bmulti_resource_scheduling%2c%2bname%2c%2bnetwork_affiliations%2c%2bnetworks%2c%2bnotes%2c%2bnpi%2c%2bother_video_url%2c%2breviews%2c%2bpersonal_interests%2c%2bphilosophy_of_care_video_url%2c%2bpractice_groups%2c%2bprimary_group_affiliation%2c%2bprofessional_associations%2c%2bprofessional_statement%2c%2bprovider_is_employed%2c%2bprovider_title%2c%2bprovider_type%2c%2bprovider_videos%2c%2bxp_publications%2c%2bresearch_pubs%2c%2bscope_of_practice%2c%2bshow_in_find-a-doc%2c%2bsitecore_guid%2c%2bsitecore8_guid%2c%2bsort_preferences%2c%2bspecialties%2c%2btraining%2c%2byears_in_practice&facet=none!&filter=networks.network%3aWAProvidence.org%7cMTProvidence.org%7cAKProvidence.org%7cORProvidence.org%7cCAProvidence.org&filter=networks.network%3aWAProvidence.org&page=1&sort=relevance';
            var queryOrder = {"name":-8,"first":-7,"middle":-6,"last":-5,"title":-4,"city":-3,"id":-2,"region":0,"search-term":1,"page":3,"caal":4,"insurance":5,"subspecialty-filter":6,"location":2,"specialty-filter":3,"acceptingNewPatients":4,"added":5,"age-adolescent":6,"age-adult":7,"age-geriatric":8,"age-pediatric":9,"deleted":10,"ids":11,"language":12,"location-id":13,"location-name":14,"medical-group":15,"mychartview":16,"name-prefix":17,"noscript":18,"npi":19,"practice-group":20,"rated":21,"reset":22,"sc-guid":23,"search-query":24,"shuffle-seed":25,"suggestion-term":26,"format":27,"gender":28,"name-part":29,"pid":30,"range":31,"rating":32,"search":33,"search-type":34,"sort":35,"suggestion-type":36};

            function cleanupInputs(form, additionalInputNames) {
                var inputNameMap = {};
                Array.from(form.elements).filter(function (input) {
                    if (!input.name)
                        return false;

                    if (!input.value)
                        return true;

                    if (additionalInputNames && additionalInputNames.indexOf(input.name) !== -1)
                        return true;

                    var existingValue = inputNameMap[input.name];
                    inputNameMap[input.name] = input.value;
                    return existingValue === input.value;
                }).forEach(function (input) {
                    input.removeAttribute('name'); // effectively disable the input from being submitted in the form
                });
            }

            function orderInputs(form) {
                var inputGroups = [];
                for (var key in queryOrder) {
                    if (queryOrder.hasOwnProperty(key)) {
                        var order = queryOrder[key];
                        var matches = Array.prototype.slice.call(document.querySelectorAll('[name="' + key + '"]'), 0);
                        matches = matches.filter(function(inout) {
                            return inout.tagName !== 'META';
                        });
                        inputGroups[order] = matches;
                    }
                }

                var frag = document.createDocumentFragment();

                inputGroups.forEach(function (inputGroup) {
                    inputGroup.forEach(function (input) {
                        var hiddenInput = document.createElement('input');
                        hiddenInput.type = 'hidden';
                        hiddenInput.name = input.name;
                        hiddenInput.value = input.type === "checkbox" ? input.checked && input.value || '' : input.value;
                        frag.appendChild(hiddenInput);
                        input.removeAttribute('name');
                    });
                });

                form.insertBefore(frag, form.firstChild);
            }

            // get user inputs (not-hidden), unless there is a checkbox or radio, in which case, hidden is
            // preferred-- ONLY when the checkbox is unchecked, or no radio of the same group is selected
            function getUserInputs(formElement) {
                var radios = {};
                var hElements = Array.from(formElement.elements).filter(function (input) {
                    return !!input.name && !input.disabled && input.type === "hidden";
                });
                var elements = Array.from(formElement.elements).filter(function (input) {
                    return !!input.name && !input.disabled && input.type !== "hidden";
                });
                var results = elements.map(function (input) {
                    if (!input.name || input.disabled || input.type === "hidden")
                        return null;

                    if (input.type === "radio") {
                        (radios[input.name] || (radios[input.name] = [])).push(input);
                        return null;
                    }

                    if (input.type === "checkbox" ? !input.checked : !input.value) {
                        // when a checkbox is unchecked, see if the unchecked
                        // state has a value
                        var hidden = hElements.find(function (input2) { return input2.type === "hidden" && input2.name === input.name });
                        return hidden ? hidden : input;
                    }

                    return input;
                }).filter(function (input) {
                    return input !== null;
                });

                for (var name in radios) {
                    if (radios.hasOwnProperty(name)) {
                        var namedRadios = radios[name];
                        var checked = namedRadios.find(function (radio) {
                            return radio.checked;
                        });
                        if (checked) {
                            results.push(checked);
                        } else {
                            var hidden = elements.find(function (input2) {
                                return input2.type === "hidden" && input2.name === name;
                            });
                            if (hidden) {
                                results.push(input);
                            } else {
                                // push all radios
                                Array.prototype.push.apply(results, namedRadios);
                            }
                        }
                    }
                }

                return results;
            }

            function inputHasValue(input) {
                if (!input.value) {
                    return false;
                }

                if (input.type === 'checkbox' || input.type === 'radio') {
                    return input.checked;
                }

                return true;
            }

            function createGuid() {
                return 'xxxxxxxx-xxxx-4xxx-yxxx-xxxxxxxxxxxx'.replace(/[xy]/g, function (c) {
                    var r = Math.random() * 16 | 0, v = c === 'x' ? r : (r & 0x3 | 0x8);
                    return v.toString(16);
                });
            }

            document.addEventListener('DOMContentLoaded', function () {

                //----- Verify Token and Initialize Typeahead -----//
                if (tkn.length < 1 || tkn == 'error') {
                    console.log("error: no auth token");
                }
                else {
                    initializeJqueryTypeahead(tkn, typeaheaduri, regionfilter, [{"Find":"abortion","Replace":null,"IgnoreCase":true,"UseRegex":null},{"Find":"ivf","Replace":null,"IgnoreCase":true,"UseRegex":null},{"Find":"in vitro fertilization","Replace":null,"IgnoreCase":true,"UseRegex":null},{"Find":"sterilization","Replace":null,"IgnoreCase":true,"UseRegex":null},{"Find":"essure","Replace":null,"IgnoreCase":true,"UseRegex":null},{"Find":"(?=^(?:(?!reversal).)*$)tubal-ligation","Replace":null,"IgnoreCase":true,"UseRegex":true},{"Find":"(?=^(?:(?!reversal).)*$)vasectomy","Replace":null,"IgnoreCase":true,"UseRegex":true},{"Find":"transgender.*surgery","Replace":"transgender care","IgnoreCase":true,"UseRegex":true}]);
                }

                //----- Search button logic -----//
                $('#search-button').click(function () {
                    // ----------------------------------------- Typeahead V.S. Unified search logic ----------------------------------------- //
                    //
                    // 1. We need to determine if this is a typeahead search
                    //    How? If window.Typeahead["#search-term"].item is not undefined or null, then the user selected a typeahead item.
                    //    Caveat - On a SERP, we won't know if it's a typeahead or unified search because the user hasn't selected anything yet.
                    //
                    // 2. If typeaheadItem exists, we do nothing and keep the current values to submit - done
                    //
                    // 3. If the typeaheadItem doesn't exist, we have more work to do.
                    //      a. Check if the search-type is = "unified". If so, it's a unified search - done.
                    //      b. See if the querystring search-term matches the input box search-term. If so, the user is filtering or searching
                    //         the exact same term. - done
                    //      c. If the terms don't match, this is a unified search - done
                    //


                    var typeaheadItem = window.Typeahead["#search-term"].item;
                    var searchQueryValue = $('#search-query').val();
                    var searchTypeValue = $('#search-type').val();
                    var isFilteredSearch = urlParams["search-term"] == $('#search-term').val() ? true : false;
                    var isUnifiedSearch = false;

                    // Does typeahead item exist?
                    if (typeaheadItem == null || typeaheadItem.value == undefined) {

                        // Typeahead Item Doesn't Exist:
                        // This could be because we're on the SERP from a typeahead search or it could be a unified search. 
                        // On the SERP, the user hasn't selected a typeahead item. We populate the input box value on page load. But...
                        // Only selecting an item from the typeahead dropdown will create an instance of the typeaheadItem.

                        // Is the search-type specified as "unified"?:
                        if (searchTypeValue != "unified" && searchTypeValue != "") {
                            // Not Specified as Unified. We may be dealing with the SERP from a typeahead search. We need more information though.

                            // Do the qs and input box values for search-term match?:
                            if (!isFilteredSearch) {
                                // No the qs and input box values don't match.  This is a unified search. 
                                isUnifiedSearch = true;
                            }
                        }

                        // A unified search-type is present
                        if (searchTypeValue == "unified" || searchTypeValue == "") {
                            // Search type was specified as "unified";
                            isUnifiedSearch = true;
                        }
                    }

                    //if this is a unified search, let's set the search-type value and set up our hidden input values to utilize on the SERP.
                    if (isUnifiedSearch) {
                        $('input[name="search-type"]').val('unified');
                        if ($('input[name="search-query"]').val() != $('input[name="search-term"]').val()) {
                            $('input[name="search-query"]').val($('input[name="search-term"]').val());
                        }
                    }
                    //----- End Typeahead V.S. Unified search logic ----- //

                    //----- Begin Sorting rules ------ //
                    var sortBy = $('#sort').val() != null && $('#sort').val() != "" ? $('#sort').val() : "relevance";
                    //if no location they can't sort by distance so don't let them.
                    if ($('#location').val() == null || $('#location').val() == "") {
                        if (urlParams["sort"] == "distance") {
                            sortBy = "relevance";
                        }
                    }
                    if ($('#location').val() != null && $('#location').val() != "") {
                        if (urlParams["location"] != $('#location').val()) {
                            //console.log("something changed - sorting by distance");
                            //value changed so lets sort by distance automatically
                            sortBy = "distance";
                        }
                    }
                    $('#sort').val(sortBy);
                    //----- End Sorting rules ------ //

                    $('#page').val("1");
                    $('#shuffle-seed').val(createGuid().replace(/-/g, ''));

                    var pendingActions = 0;
                    var formElement = document.getElementById('typeahead-search');
                    var doFormSubmit = function () {
                        if (--pendingActions !== 0) {
                            return;
                        }

                        try {
                            // remove unused inputs to cleanup query string
                            // make sure to include removed input names-- we don't want any injected hidden values taking precendence in the query string
                            cleanupInputs(formElement);
                            orderInputs(formElement);

                            //wheelhouse analytics binding function
                            if (typeof (window.wheelhouseFormAnalytics) == "function") {
                                window.wheelhouseFormAnalytics(formElement);
                            }
                            formElement.submit();

                        } catch (e) {
                            formSubmitting = false;
                            throw e;
                        }
                    };

                    pendingActions++;
                    tealiumHelper.push('link', {
                        'event_category': 'Conversion Level 2',
                        'event_action': 'Provider Search',
                        'event_label': 'true'
                    }).then(doFormSubmit, doFormSubmit);

                    var addedInputs = getUserInputs(formElement).filter(inputHasValue).filter(function (input) {
                        return urlParams[input.name] !== input.value;
                    }).reverse().filter(function (value, index, self) {
                        for (var i = 0; i < self.length; i++) {
                            if (self[i].name === value.name) {
                                return i === index;
                            }
                        }
                        return false;
                    }).reverse();

                    if (addedInputs.length) {
                        addedInputs.forEach(function (input) {
                            pendingActions++;
                            var name = input.name;
                            var value = input.value;
                            if (input.name == 'sort') {
                                name = 'sort by';
                                
                                if (value == 'name') {
                                    value = 'providers a-z';
                                } else if (value == '-name') {
                                    value = 'providers z-a';
                                }
                            }
                            if (input.name == 'range') {
                                if (value == '3500') {
                                    value = 'any distance';
                                }
                            }
                            tealiumHelper.push('link', {
                                'event_category': 'Provider Search',
                                'event_action': name,
                                'event_label': value
                            }).then(doFormSubmit, doFormSubmit);
                        });
                    } else {
                        pendingActions++;
                        tealiumHelper.push('link', {
                            'event_category': 'Provider Search',
                            'event_action': 'No Selection',
                            'event_label': 'No Selection'
                        }).then(doFormSubmit, doFormSubmit);
                    }
                });

                //----- Input box click handlers -----//
                $("#search-term").keyup(function (event) {
                    if (event.keyCode == 13) {
                        $("#search-button").click();
                    }
                });
                $("#location").keyup(function (event) {
                    if (event.keyCode == 13) {
                        $("#search-button").click();
                    }
                });
            });
        })();
    </script>
