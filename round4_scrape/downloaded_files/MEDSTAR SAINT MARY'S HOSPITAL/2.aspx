				jQuery('#hero-video').get(0).pause();
				jQuery('.header-section.hero-video-toggle').removeClass('video-playing');
			}
		});
	});

	// Watch youtube modal opening to determine what video content to render
	jQuery('#youtube-modal').on('show.bs.modal', function(e) {
		//get data-id attribute of the clicked element
		var relatedTarget = jQuery(e.relatedTarget);
		var headerText = relatedTarget.data('header-text');
		var footerText = relatedTarget.data('footer-text');
		var youtubeLink = relatedTarget.data('youtube-link');

		//populate the data in the modal
		var currentTarget = jQuery(e.currentTarget);
		currentTarget.find('.header-text').html(headerText);
		currentTarget.find('.footer-text').html(footerText);
		currentTarget.find('.youtube-iframe').attr('src', youtubeLink + '?autoplay=1&rel=0');
	});

	// Modal closed, destroy the iframe in case the video is still running
	jQuery('#youtube-modal').on('hide.bs.modal', function(e) {
		var currentTarget = jQuery(e.currentTarget);
		currentTarget.find('.youtube-iframe').attr('src', 'about:blank');
	});

	</script>
