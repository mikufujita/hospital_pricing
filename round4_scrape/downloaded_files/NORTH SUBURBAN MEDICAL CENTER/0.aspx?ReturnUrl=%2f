<script type="text/javascript">
    function isCapsLockOn(e) {
        var keyCode = e.keyCode ? e.keyCode : e.which;
        var shiftKey = e.shiftKey ? e.shiftKey : ((keyCode == 16) ? true : false);
        return (((keyCode >= 65 && keyCode <= 90) && !shiftKey) || ((keyCode >= 97 && keyCode <= 122) && shiftKey))
    }
 
     jQuery(document).ready(function () {
         document.msCapsLockWarningOff = true;  //disable IE Caps Lock warning
         jQuery(":password").keypress(function (e) {
             if (isCapsLockOn(e))
                 jQuery("#capsLockWarning").show();
             else
                 jQuery("#capsLockWarning").hide();
         });

         jQuery("[id$='txtUsername']").attr('autocomplete','off');
         jQuery("[id$='txtPassword']").attr('automplete','off');
         jQuery("[id$='txtDomain']").attr('autocomplete','off');

         jQuery("[id$='txtUsername']").attr('value','');
         jQuery("[id$='txtPassword']").attr('value','');
         jQuery("[id$='txtDomain']").attr('value','');

     });


</script>
   

