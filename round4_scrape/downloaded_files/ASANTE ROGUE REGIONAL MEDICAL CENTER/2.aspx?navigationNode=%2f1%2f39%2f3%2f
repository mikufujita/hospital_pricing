<script type="text/javascript">
//<![CDATA[

function clearDefaultValue(objectId,defaultValue,cssClass) {
	var jqObj = $("#"+objectId);
	if (jqObj.val() == defaultValue) {
		jqObj.val("");
	}

	jqObj.removeClass(cssClass);
}

function addBackDefaultValue(objectId,defaultValue,cssClass) {
	var jqObj = $("#"+objectId);
	if (jqObj.val() == "") {
		jqObj.val(defaultValue);
		jqObj.addClass(cssClass);
	}
}
//]]>
</script>

