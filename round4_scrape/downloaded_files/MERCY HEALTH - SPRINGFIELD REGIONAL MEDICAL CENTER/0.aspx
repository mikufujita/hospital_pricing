                                tmpValues = tmpValues + "'" + $('#fldpdob').val() + '/' + $('#patient_dob_day').val() + '/' + $('#patient_dob_year').val() + "'" + ',';
                            } else if ($(this).attr('name') == 'fldnrphone') {
                                tmpFields = tmpFields + $(this).attr('name') + ',';
                                tmpValues = tmpValues + "'" + $('#fldnrphone').val() + '-' + $('#relative_phone1').val() + '-' + $('#relative_phone2').val() + "'" + ',';

                            // step 2
                            } else if ($(this).attr('name') == 'fldpempphone') {
                                tmpFields = tmpFields + $(this).attr('name') + ',';
                                tmpValues = tmpValues + "'" + $('#fldpempphone').val() + '-' + $('#patient_employer_phone1').val() + '-' + $('#patient_employer_phone2').val() + "'" + ',';

                            // step 3
                            } else if ($(this).attr('name') == 'fldrpphone') {
                                tmpFields = tmpFields + $(this).attr('name') + ',';
                                tmpValues = tmpValues + "'" + $('#fldrpphone').val() + '-' + $('#responsible_party_phone1').val() + '-' + $('#responsible_party_phone2').val() + "'" + ',';
                            } else if ($(this).attr('name') == 'fldrpdob') {
                                tmpFields = tmpFields + $(this).attr('name') + ',';
                                tmpValues = tmpValues + "'" + $('#fldrpdob').val() + '/' + $('#responsible_party_dob_day').val() + '/' + $('#responsible_party_dob_year').val() + "'" + ',';

                            // step 4
                            } else if ($(this).attr('name') == 'fldrpempphone') {
                                tmpFields = tmpFields + $(this).attr('name') + ',';
                                tmpValues = tmpValues + "'" + $('#fldrpempphone').val() + '-' + $('#responsible_party_employer_phone1').val() + '-' + $('#responsible_party_employer_phone2').val() + "'" + ',';

                            // step 5
                            } else if ($(this).attr('name') == 'fldadmissiondate') {
                                tmpFields = tmpFields + $(this).attr('name') + ',';
                                tmpValues = tmpValues + "'" + $('#fldadmissiondate').val() + '/' + $('#injury_admin_day').val() + '/' + $('#injury_admin_year').val() + "'" + ',';
                            } else if ($(this).attr('name') == 'flddateofinjury') {
                                tmpFields = tmpFields + $(this).attr('name') + ',';
                                tmpValues = tmpValues + "'" + $('#flddateofinjury').val() + '/' + $('#injury_dob_day').val() + '/' + $('#injury_dob_year').val() + "'" + ',';

                            } else {
                                if ($(this).hasClass('Skip') == false) {
                                    tmpFields = tmpFields + $(this).attr('name') + ',';
                                    tmpValues = tmpValues + "'" + $(this).val().replace("'","''") + "'" + ',';
                                }
                            }
                        }
                    }
                });
                $('#fldStep' + currentStep).html('');
                $('#valStep' + currentStep).html('');
                $('#fldStep' + currentStep).html(tmpFields);
                $('#valStep' + currentStep).html(tmpValues);

                if ((currentStep == 3 && $('#fldrpname').val() == '') && gt == 4) {
                    gt = 5;
                }
                if ((currentStep == 5 && $('#fldrpname').val() == '') && gt == 4) {
                    gt = 3;
                }
                if (gt == 6 && editSI == '') {
                    currentStep = 6;
                    $('.navImage').hide();
                    $('#navMessage').addClass('alertColor');
                    $('#navMessage').html('Please review and acknowledge the information in the sections below.<br />After acknowledging each section click the Submit Form button at the bottom of the page.');
                    $('.insMsg').hide();
                    $('#tdRow').hide();
                    $('#step1').fadeIn()
                    $('#step2').fadeIn()
                    $('#step3').fadeIn()
                    if ($('#fldrp').find(":checked").val() == 'No') {
                        $('#step4').fadeIn()
                    }
                    $('#step5').fadeIn()
                    $('.btUpdate').show();

                }else if(editSI != ''){
                    $('#' + editSI).hide();
                    $('.S' + editSI).each(function () {
                        $(this).prop("disabled", true);
                    });
                    if (editSI == 3 && ($('#fldrpname').val() != '' && (setRPSame != $('#fldrp').find(":checked").val()))) {
                        editSI = '';
                        $('#step4').fadeIn()
                        $('#e3').show();
                        $('#4').show();
                    } else if (editSI == 3 && ($('#fldrpname').val() == '')) {
                        $('#4').show();
                        $('#e4').hide();
                        $('.S4').each(function () {
                            $(this).prop("disabled", false);
                        });
                        $('#step4').hide();
                        $('#e3').show();
                        $('#fldStep4').val('');
                        $('#valStep4').val('');
                    } else {
                        $('#e' + editSI).show();
                        editSI = '';
                    }
                    showSubmit = '';
                    $('.btUpdate').each(function () {
                        if ($(this).is(":visible") == true) {
                            showSubmit = 'no';
                        }
                    });
                    if (showSubmit == 'no') {
                        $('#btSubmitForm').hide();
                    } else {
                        $('#btSubmitForm').fadeIn();
                    }
                } else {
                    $('#step' + currentStep).hide()
                    $('#step' + gt).fadeIn()
                }
                cmdLoadSteps(gt)
            } else {
                $('#alertAncor' + currentStep).html('Please verify all required fields have been completed.');
                $('#alertAncor' + currentStep).fadeIn();
            }
        }
        
        function cmdLoadSteps(s) {
            currentStep = s;
            strRow = "";
            $('#tdRow').html('');
            i = 1;
            while (i < 6) {
                if (i == s) {
                    strRow = strRow + "<td><img src=images/step" + i + "_selected.png border=\"0\"></td>";
                } else {
                    strRow = strRow + "<td><img src=images/step" + i + ".png border=\"0\"></td>";
                }
                i++;
            }
            $('#tdRow').append(strRow);           
        }

        function editSection(si) {
            editSI = si;
            $('#e' + si).hide();
            $('.S' + si).each(function () {
                $(this).prop("disabled", false);
            });
            setRPSame = $('#fldrp').find(":checked").val();
            $('#' + editSI).show();
        }

        $('.btUpdate').click(function () {
            editSI = $(this).attr('id');
            currentStep = editSI;
            cmdStepTo(6);
        });

        $('.facilityRow').mouseover(function () {
            $(this).addClass('mouseOn');
        });
        $('.facilityRow').mouseleave(function () {
            $(this).removeClass('mouseOn');
        });
        
        function delconfirm() {
            var yesno = confirm("You must indicate that you accept the terms of the disclaimer to continue.\n\nClick Cancel to try again,\n\nor\n\nClick OK to exit the preregistration system.");
            if (yesno == true) {
                $('#tblDisclaimer').fadeOut();
                $('#tblSteps').fadeIn();
            }
            else {
                return false;
            }
        }

        $('.NumberCheck').keyup(function (e) {
            numFldCheck = '';
            code = e.keyCode ? e.keyCode : e.which;
            var value = $(this).val();
            var fldMax = $(this).attr('maxLength');
            var fldLen = $(this).val().length;
            if (code == 32) {
                alert('Blank spaces are not allowed.');
                $(this).val('');
                $(this).focus();
            }
            if (!jQuery.trim(value) == '' && !jQuery.isNumeric(value)) {
                alert('Please enter a numeric value');
                $(this).val('');
                $(this).focus();
            }
            if ($(this).hasClass('hourCheck') == true && $(this).val().length > 0) {
                if ($(this).val() > 12 || $(this).val()*1 < 1) {
                    alert('Please enter a hour between 1 and 12');
                    $(this).val('');
                    $(this).focus();
                }
            }
            if ($(this).hasClass('minCheck') == true && $(this).val().length > 0) {
                if ($(this).val() > 59 || $(this).val() * 1 < 0) {
                    alert('Please enter a hour between 1 and 12');
                    $(this).val('');
                    $(this).focus();
                }
            }
            if ($(this).hasClass('birthYear') == true && $(this).val().length >= 4) {
                if ($(this).val() > currentYear || $(this).val() < 1900) {
                    alert('Please enter a valid year');
                    $(this).val('');
                    $(this).focus();
                }
            }            
        });

     //   $('.NumberCheck').focusout(function () {
     //       if ($(this).val().length > 1 && ($(this).val().length < $(this).attr('maxlength'))) {
     //           alert('Please check the length of the value entered');
    //            $(this).focus();
    //        }
   //     });

        //step 2

        $('.divOccupationClick').click(function () {
            $(this).removeClass('fldError');
            $(this).removeClass('fldErrorFont');
            $('.showAlertMsg').hide();
            if ($(this).find(":checked").val() == 'Yes') {
                $('#divOccupationNo').hide();
                $('#divOccupationYes').fadeIn();
            } else {
                $('#divOccupationYes').hide();
                $('#divOccupationNo').fadeIn();
            }
        });

        // shared with step 2 and 4
        $('.jobStatus').change(function () {
            $(this).removeClass('fldError');
            $(this).removeClass('fldErrorFont');
            $('.showAlertMsg').hide();
            getVal = $(this).val();
            getID = $(this).attr('id');
            if (getVal == 'Retired') {
                if (getID == 'fldpatientemployerjobstatus') {
                    $('#ptrDate').fadeIn();
                }
                if (getID == 'fldresponsiblepartyemployerjobstatus') {
                    $('#rprDate').fadeIn();
                }
            } else {
                if (getID == 'fldpatientemployerjobstatus') {
                    $('#ptrDate').hide();
                }
                if (getID == 'fldresponsiblepartyemployerjobstatus') {
                    $('#rprDate').hide();
                }
            }
        });

        //step 3

        $('.divResponsibleClick').click(function () {
            if ($(this).find(":checked").val() == 'Yes') {
                $('#divResponsibleYes').hide();
            } else {
                $('#divResponsibleYes').fadeIn();
            }
            $(this).removeClass('fldError');
            $('.showAlertMsg').hide();
        });

        // step 4

        $('.resSamePartyClick').click(function () {
            if ($(this).find(":checked").val() == 'No') {
                $('#divResEmployed').fadeIn();
                if ($('#fldrpempemployed').find(":checked").val() == 'No') {
                    $('#divEmployedYes').hide();
                    $('#divEmployedNo').fadeIn();
                }
                if ($('#fldrpempemployed').find(":checked").val() == 'Yes') {
                    $('#divEmployedYes').fadeIn();
                    $('#divEmployedNo').hide();
                }
            } else {                
                $('#divResEmployed').hide();
                $('#divEmployedYes').hide();
                $('#divEmployedNo').hide();                
            }
            $(this).removeClass('fldError');
            $('.showAlertMsg').hide();
        });

        $('.divEmployedClick').click(function () {
            if ($(this).find(":checked").val() == 'Yes') {
                $('#divEmployedYes').fadeIn();
                $('#divEmployedNo').hide();
            } else {
                $('#divEmployedYes').hide();
                $('#divEmployedNo').fadeIn();
            }
            $(this).removeClass('fldError');
            $('.showAlertMsg').hide();
        });


        // step 5

        $('.divInjuryRelatedClick').click(function () {
            if ($(this).find(":checked").val() == 'Yes') {
                $('#divInjuryRelatedYes').fadeIn();
            } else {
                $('#divInjuryRelatedYes').hide();
            }
            $(this).removeClass('fldError');
            $('.showAlertMsg').hide();
        });

        $('.insuranceClick').click(function () {
            if ($(this).find(":checked").val() == 'Yes') {
                $('.insuranceYes').slideDown();
            } else {
                $('.insuranceYes').hide();
            }
            $(this).removeClass('fldError');
            $('.showAlertMsg').hide();
        });

        $('.secinsuranceClick').click(function () {
            if ($(this).find(":checked").val() == 'Yes') {
                $('.secinsuranceYes').slideDown();
            } else {
                $('.secinsuranceYes').hide();
            }
            $(this).removeClass('fldError');
            $('.showAlertMsg').hide();
        });

        $('.medicarePTClick').click(function () {
            if ($(this).find(":checked").val() == 'Yes') {
                $('.medicarePTYes').slideDown();
            } else {
                $('.medicarePTYes').hide();
            }
            $(this).removeClass('fldError');
            $('.showAlertMsg').hide();
