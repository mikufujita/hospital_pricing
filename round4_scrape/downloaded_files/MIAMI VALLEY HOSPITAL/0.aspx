<script type="text/javascript" charset="utf-8">
    $(document).ready(function () {

        $("#rotator").owlCarousel({
            navigation: false,
            slideSpeed: 300,
            pagination:false,
            singleItem: true,
            autoPlay: 5000    //false to turn off, integer for speed
        });

        lazyLoad_handler();
    });

    lazyLoad_handler = function () {

        var elements = document.getElementsByClassName("rotatorSlide");

        var counter = 0;
        setURL(counter); // no need for timeout here

        if (elements.length === 1) {
            elements[0].removeAttribute("realsrc");
            elements[0].removeAttribute("src");
            return;
        }
        counter++;

        timer = setInterval(function () {

            if (counter === elements.length) {
                clearInterval(timer);
                for (var i = 0; i < elements.length; i++) {
                    elements[i].removeAttribute("realsrc");
                    elements[i].removeAttribute("src");
                }
            }
            else {
                setURL(counter);
                counter++;
            }
        }, 4000);


    };



    function setURL(i) {
        var elements = document.getElementsByClassName("rotatorSlide");

        if (elements[i].getAttribute("realsrc") != null) {
            elements[i].style.backgroundImage = "url('" + elements[i].getAttribute("realsrc") + "')";
        }
        else if (elements[i].getAttribute("src") != null) {
            elements[i].style.backgroundImage = "url('" + elements[i].getAttribute("src") + "')";
        }

    }

</script>


 
 

