<script type="text/javascript">
$(document).ready(function(){
	// Start the main flyout panel.
	$('#TopNavModule').flyoutpanel();

	// Set up the center menu show/hide sub-panels.
	$('#CentersMenu1').bind('mouseover',function(e){
		var link = $.getLinkTarget(e);
		if ( link && !link.is('.active') ) {
			link.parent().children('.active').removeClass('active');
			link.addClass('active');
	
			var href = link.attr('href'),
				title = link.attr('_title'),
				description = $.decode( link.attr('_description') || "" ),
				sb = [];
			
			sb.push( description );
			sb.push( ' <a href="' );
			sb.push( href );
			sb.push( '">Read More [+]</a>' );
			
			link.closest('div.services1')
				.next('div.sub-text')
					.find('div.Sub-Panel')
						.html( sb.join("") );
		}
	});
});
</script>

