<script type="text/javascript">
//<![CDATA[
var getQueryParam = function getQueryParam(param) {
	var query = window.location.search.substring(1);
	var vars = query.split("&");
	for (var i = 0; i < vars.length; i++) {
		var nvPair = vars[i].split("=");
		if (nvPair[0] === param) {
			return decodeURIComponent(nvPair[1]);
		}
	}
	return "";
};

$(document).ready(function () {

	//change the email address
	$('#FormDetail_FormDescription a[href*="mailto"]').attr("href", "mailto:DLSYSInternetGovernance@peacehealth.org");

	//change the 'OK' button to say "Send Feedback"
	$('.OKButton').val("Send Feedback");

	//visible fields
	var medCtrField = $('#FormDetail_Question68861');

	//diagnostic fields
	var referrerField = $('#FormDetail_Question68851');
	var referrerPageTitleField = $('#FormDetail_Question68864');
	var userAgentField = $('#FormDetail_Question68852');
	var documentModeField = $('#FormDetail_Question68853');
	var localeField = $('#FormDetail_Question68857');
	var cookiesEnabledField = $('#FormDetail_Question68858');
	var screenResolutionField = $('#FormDetail_Question68862');
	var ipField = $('#FormDetail_Question68863');


	//do some things when in user mode, but not when reviewing responses
	if (window.location.href.toLowerCase().indexOf("form.aspx") >= 0 || window.location.href.toLowerCase().indexOf("default.aspx") >= 0) {

		//list the referring page title and URL
		referrerField.val(document.referrer);
		referrerPageTitleField.val(getQueryParam("title"));

		//pre-select medical center based on referring URL
		var medCtrKeys = {
			"/ketchikan": "378774", 
			"/ketchikan-medical-center": "378774", 
			"/cottage-grove": "378775", 
			"/cottage-grove-medical-center": "378775", 
			"/peace-harbor": "378776", 
			"/peace-harbor-medical-center": "378776", 
			"/sacred-heart-riverbend": "378777", 
			"/sacred-heart-university-district": "378778", 
			"/st-john": "378779", 
			"/st-john-medical-center": "378779", 
			"/st-joseph": "378780", 
			"/st-joseph-medical-center": "378780", 
			"/peace-island": "378781", 
			"/peace-island-medical-center": "378781", 
			"/southwest": "378782", 
			"/southwest-medical-center": "378782", 
      "/united-general": "378783", 
      "/united-general-medical-center": "378783"
		};
		for (var key in medCtrKeys) {
			if (medCtrKeys.hasOwnProperty(key)) {
				if (document.referrer.indexOf(key) >= 0) {
					try {
						var option = $('option[value="' + medCtrKeys[key] + '"]');
						var optionIndex = option.get(0).index;
						medCtrField.get(0).selectedIndex = optionIndex;
						break;
					} catch (exc) {
						//no-op
					}
				}
			}
		}


		//collect user agent diagnostics

		//user agent
		userAgentField.val(navigator.userAgent);

		//IE document mode
		if (navigator.userAgent.indexOf("MSIE") >= 0 && document.documentMode) {
			documentModeField.val(document.documentMode);
		} else {
			documentModeField.val("N/A");
		}

		//locale
		var locale = window.navigator.userLanguage || window.navigator.language;
		localeField.val(locale);

		//cookies enabled?
		cookiesEnabledField.val(navigator.cookieEnabled);

		//screen resolution
		try {
			screenResolutionField.val(screen.width + "\u00D7" + screen.height);
		} catch (exc) {
			//no-op
		}

		//IP field
		var ipVal = "";
		if (window.location.search.length >= 1) {
			var ipVal = getQueryParam("i");
			ipVal = ipVal.replace(/%/g, ".");
			ipField.val(ipVal);
		}


		//hide diagnostic fields
		$(':contains("User Agent Diagnostic Information")').closest("tr").css("display", "none");
		var fieldsToHide = [referrerField, referrerPageTitleField, localeField, cookiesEnabledField, userAgentField, documentModeField, screenResolutionField, ipField];
		$.each(fieldsToHide, function (index, item) {
			item.attr("readonly", "readonly");
			item.parents('tr').css("display", "none");
		});

		//hide the "form description" label
    $('#FormDetail_FormDescriptionLabel').css("display", "none");
		//hide the 'OK/Save' button that displays above the form
		$('#FormDetail_SaveRow2').css("display", "none");

    $('#FormDetail_DirectionsLabel').css("margin-top", "1em");

    //modify the link to the Content Update form to include the original referrer
    //https://app.peacehealth.org/Form/default.aspx?FormID=5265
    var contentUpdateLink = $('a[href*="5265"]');
    var contentUpdateUrl;
    if (document.referrer) {
      contentUpdateUrl = contentUpdateLink.attr("href");
      contentUpdateUrl += "&referrer=" + encodeURI(document.referrer);
      contentUpdateLink.attr("href", contentUpdateUrl);
    }
    if (referrerPageTitleField.val()) {
      contentUpdateUrl = contentUpdateLink.attr("href");
      contentUpdateUrl += "&title=" + encodeURI(referrerPageTitleField.val());
      contentUpdateLink.attr("href", contentUpdateUrl);
    }


		
	} else {
		var uaIdentifierSrc = "http://www.useragentstring.com/?uas=" + encodeURI(userAgentField.val()) + "&getText=all";
		var uaIdentifierLink = $('<br/><a id="userAgentInfo" href="' + uaIdentifierSrc + '" target="_blank">User Agent Details</a>');
		userAgentField.after(uaIdentifierLink);
	}
});

//]]>
</script><script type="text/javascript">$(document).ready(function(){});</script>
