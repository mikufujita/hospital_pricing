     var pageName = (function () {
         var a = window.location.href,
             b = a.lastIndexOf("/");
         return a.substr(b + 1);
     }());
     //console.log("page name: " + pageName);
    if (pageName.indexOf("profile.aspx") > -1 && urlParams["mychartview"] == "yes"){ // window.location.href.indexOf("mychartview=yes") > -1) { //["mychartview"] == "yes") {
