        $('.emailCheck').focusout(function () {
            var emailReg = /^([\w-\.]+@([\w-]+\.)+[\w-]{2,4})?$/;
            if (emailReg.test($(this).val()) == false) {
                alert('Please enter a valid email address.');
                $(this).val('');
                $(this).focus();
            }
        });

        function cmdSave() {
            var flds = $('#fldStep1').html() + $('#fldStep2').html() + $('#fldStep3').html() + $('#fldStep4').html() + $('#fldStep5').html() + 'fldfacility';
            var vals = $('#valStep1').html() + $('#valStep2').html() + $('#valStep3').html() + $('#valStep4').html() + $('#valStep5').html() + "'" + getFacility + "'";
            $('#bt_SubmitForm').prop('value','Processing, please wait');
            $('#bt_SubmitForm').prop("disabled", true);
            $.post("scripts/saveprereg.aspx",
                {
                    strSQLFields: flds,
                    strSQLValues: vals,
                    strQLFacility: getFacility
                },
                function (data) {
                    window.location = 'complete.aspx';
                })
                .error(function () {
                    alert('There was an issue, please try and submit again.');
                    $('#bt_SubmitForm').prop('value','Submit Form');
                    $('#bt_SubmitForm').prop("disabled", false);
                });
        }

        agree();

    </script>
