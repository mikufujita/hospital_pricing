}
function getHashParameter() {
  var qs = location.search.substring(1, location.search.length);
  if (qs.length == 0) { return ""; }
  var args = qs.split('&');
  for (var i = 0; i < args.length; i++) {
    var pair = args[i].split('=');
    if ((pair.length == 2) && (pair[0] == "hashFragment")) {
      return pair[1];
    }
  }
}
