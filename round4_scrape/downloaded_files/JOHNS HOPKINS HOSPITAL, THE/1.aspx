                var div2 = document.getElementById("uploadDiv");
                div2.style.display = "block";
            }

            function OnFileSelected(sender, args) {
                var input = args.get_fileInputField();
                if (input.value.indexOf("&") > 0) {
                    alert("Files with the ampersand symbol are not allowed. Please rename your file then upload.");
                    sender.deleteAllFileInputs();
                }
                else {
                    fileUploading = true;
                }
            }

            function reloadIFrame() {
                if (window.location.href.indexOf("adventhealth") > -1) {
                    window.frames["hidden"].src = "Assets/adventhealth/images/adventhealth_newlogo.png";
                }
            }

            window.onbeforeunload = unloadMessage;
            function unloadMessage() {
                message = "A file is currently being uploaded, are you sure you want to stop the upload?"
                if (fileUploading)
                    return message;
            }

            function getParameterByName(name) {
                var match = RegExp('[?&]' + name + '=([^&]*)').exec(window.location.search);
                return match && decodeURIComponent(match[1].replace(/\+/g, ' '));
            }

            function changetranscriptdates() {
                // get the end date of the calendar right now
                var b = $("#datepickerstart").datepicker("getDate");
                var e = $("#datepickerend").datepicker("getDate");
                var bdate = b.getMonth() + 1 + "/" + b.getDate() + "/" + b.getUTCFullYear();
                var edate = e.getMonth() + 1 + "/" + e.getDate() + "/" + e.getUTCFullYear();
                location.href = "default.aspx?P=500&Begin=" + bdate + "&End=" + edate;
            }

            function changeexternaldates() {
                // get the end date of the calendar right now
                var b = $("#datepickerstart").datepicker("getDate");
                var e = $("#datepickerend").datepicker("getDate");
                var bdate = b.getMonth() + 1 + "/" + b.getDate() + "/" + b.getUTCFullYear();
                var edate = e.getMonth() + 1 + "/" + e.getDate() + "/" + e.getUTCFullYear();
                location.href = "default.aspx?P=1600&Begin=" + bdate + "&End=" + edate;
            }

            function ExportToExcel() {
                var currentwindow = window.location.href
                window.location.href = currentwindow + "&Export=True"
            }

            function rssregistration(eventid, userid) {
                $("html, body").animate({ scrollTop: 0 }, "slow");
                var oWnd = radopen("WebService/RSSRegistration.aspx?UserID=" + userid + "&EventID=" + eventid + "&ReturnURL=default.aspx?P=400&type=Regular", null);
                oWnd.SetSize(400, 250);
                oWnd.set_centerIfModal(false);
                oWnd.set_modal(true);
                oWnd.MoveTo(100, 120);
                oWnd.set_initialBehaviors(Telerik.Web.UI.WindowBehaviors.None); // set the desired behaviours  
                oWnd.set_behaviors(Telerik.Web.UI.WindowBehaviors.None); //set the desired behaviours  
                oWnd.add_close(reloadcalendar);
            }

            function viewsupportingdoc(url) {
                var oWnd = radopen(url, null)
                oWnd.SetSize(650, 400);
                oWnd.set_centerIfModal(false);
                oWnd.set_modal(true);
                oWnd.MoveTo(100, 120);
            }

            function reloadcalendar() {
                var qs = window.location.search.substring(1);

                if (qs.toLowerCase().includes("type=")) {
                    var typeqs = qs.toLowerCase().split("type=");
                    var type = typeqs[1];


                    window.location.href = "../default.aspx?P=400&type=" + type;
                }
                else if (qs.toLowerCase().includes("p=6")) {
                    var eidqs = qs.toLowerCase().split("eid=");
                    var eid = eidqs[1];


                    window.location.href = "../default.aspx?P=6&eid=" + eid;
                }
                else if (qs.toLowerCase().includes("month=")) {
                    var monthqs = qs.toLowerCase().split("month=");
                    var month = monthqs[1];


                    window.location.href = "../default.aspx?P=400&month=" + month;
                }
                else {
                    window.location.href = "../default.aspx?P=400";
                }
            }

            function ShowCompletedTests() {
                var elements = document.getElementsByClassName("hideshowComp")

                for (var i = 0; i < elements.length; i++) {
                    if (elements[i].style.display == "none") {
                        elements[i].style.display = "block";
                    } else {
                        elements[i].style.display = "none";
                    }
                }
            }
        </script>
        
        
        
        
        
        
    

