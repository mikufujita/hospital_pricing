<script type="text/javascript">
function ValidCreditCard(val)
{
	var input = document.getElementById(val.controltovalidate);
	var cc = input && input.value;
	if ( !cc ) {
		return ( val.isvalid = true );
	} else if ( cc[0] === 'x' && cc === input.getAttribute('originalvalue') ) {
		return ( val.isvalid = true );
	} else if ( /\D/g.test(cc) ) {
		return ( val.isvalid = false );
	}

	// Make sure it at least 15 digits.
	if ( cc.length < 15 ) {
		return (val.isvalid=false);
	}

	var number;
	var total = 0;
	for ( var i = cc.length-1; i >= 0; i-- ) {
		total += parseInt(cc.charAt(i));
		i--;
		if (i>=0) {
			number = String((cc.charAt(i)*2));
			for (var j=0;j<number.length;j++) {
				total += parseInt(number.charAt(j));
			}
		}
	}
	return ( val.isvalid = total % 10 == 0);
}
</script>
