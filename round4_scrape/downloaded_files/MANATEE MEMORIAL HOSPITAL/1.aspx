        var lazyLoad = true;
        const images = document.getElementsByClassName("lazy-load");
        const config = {
            rootMargin: '50px 0px',
            threshold: 0.01
        };

        let observer = null;        
        if (!('IntersectionObserver' in window) || !lazyLoad) {
            //images.forEach(preloadImage);            
